[
    {
        "name": "Admin - List Users",
        "method": "GET",
        "url": "{{baseUrl}}/api/admin/users?page=1&limit=20&search="
    },
    {
        "name": "Admin - Get User Detail",
        "method": "POST",
        "url": "{{baseUrl}}/api/admin/users/detail",
        "body": {
            "id": "{{userId}}"
        }
    },
    {
        "name": "Admin - Get Orders of User",
        "method": "POST",
        "url": "{{baseUrl}}/api/admin/users/orders",
        "body": {
            "id": "{{userId}}",
            "page": 1,
            "limit": 20,
            "status": "",
            "paymentStatus": ""
        }
    },
    {
        "name": "Admin - Update User Status",
        "method": "PUT",
        "url": "{{baseUrl}}/api/admin/users/status",
        "body": {
            "id": "{{userId}}",
            "status": "blocked"
        }
    },
    {
        "name": "Admin - Update User Roles",
        "method": "PUT",
        "url": "{{baseUrl}}/api/admin/users/roles",
        "body": {
            "id": "{{userId}}",
            "roles": [
                "customer"
            ]
        }
    },
    {
        "name": "Admin - List All Orders",
        "method": "GET",
        "url": "{{baseUrl}}/api/admin/orders?page=1&limit=20&status=&paymentStatus=&user=&method=&startDate=&endDate="
    },
    {
        "name": "Admin - Revenue Reports (by day)",
        "method": "GET",
        "url": "{{baseUrl}}/api/admin/reports/revenue?groupBy=day&startDate=&endDate="
    },
    {
        "name": "Admin - Order Statistics",
        "method": "GET",
        "url": "{{baseUrl}}/api/admin/reports/orders?startDate=&endDate="
    },
    {
        "name": "Admin - Dashboard Overview",
        "method": "GET",
        "url": "{{baseUrl}}/api/admin/dashboard"
    }
]